{
  "manifest_version": 3,
  "name": "Password Generator",
  "version": "1.0",
  "description": "Generate strong passwords and store them securely.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.png.jpeg",
      "48": "img/icon16.png.jpeg",
      "128": "img/icon16.png.jpeg"
    }
  },
  "icons": {
    "16": "img/download.jpeg",
    "48": "img/download.jpeg",
    "128": "img/download.jpeg"
  }
}
